config_version=5

[application]

config/name="Arcane Quest"
config/description="Game where you go on an adventure to collect orbs and save the world"
config/version="v0.1.0"
run/main_scene="res://Scenes/main_menu.tscn"
run/disable_stdout=true
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

